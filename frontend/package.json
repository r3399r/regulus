{
  "name": "regulus-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env BROWSER=none PORT=3000 react-scripts start",
    "build": "cross-env BUILD_PATH='./dist' react-scripts build",
    "test": "react-scripts test --coverage",
    "test:nowatch": "npm run test -- --watchAll=false",
    "eject": "react-scripts eject",
    "lint:es": "eslint -c ./eslint.json src",
    "fix:es": "npm run lint:es -- --fix",
    "lint:format": "prettier --list-different \"{src,public}/**/*.{tsx,ts,js,html,css,json}\"",
    "fix:format": "npm run lint:format -- --write",
    "lint": "run-s lint:*",
    "fix": "run-s fix:*",
    "clean": "del-cli dist -f",
    "pre:commit": "npm run lint"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.14.7",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.5.0",
    "better-react-mathjax": "^2.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.15.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.46",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "cross-env": "^7.0.3",
    "del-cli": "^5.0.1",
    "eslint": "^8.48.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "http-proxy-middleware": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "react-scripts": "5.0.1",
    "tailwindcss": "^3.3.3",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
